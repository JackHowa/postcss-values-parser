{
  "name": "postcss-values-parser",
  "version": "6.0.2",
  "description": "A CSS property value parser for use with PostCSS",
  "license": "MPL-2.0",
  "repository": "shellscape/postcss-values-parser",
  "author": {
    "name": "Andrew Powell (shellscape)",
    "email": "andrew@shellscape.org",
    "url": "http://shellscape.org"
  },
  "homepage": "https://github.com/shellscape/postcss-values-parser",
  "bugs": "https://github.com/shellscape/postcss-values-parser/issues",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "ci:coverage": "nyc npm run test && nyc report --reporter=text-lcov > coverage.lcov",
    "ci:test": "npm run test",
    "lint": "eslint src test --fix --cache",
    "lint-staged": "lint-staged",
    "security": "npm audit --audit-level=high --prod",
    "test": "ava test/integration.ts"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "color-name": "^1.1.4",
    "css-tree": "^2.1.0",
    "is-url-superb": "^4.0.0",
    "quote-unquote": "^1.0.0",
    "ts-node": "^10.8.2"
  },
  "devDependencies": {
    "@types/color-name": "^1.1.1",
    "@types/css-tree": "^1.0.7",
    "@types/node": "^18.0.3",
    "ava": "^4.3.0",
    "chalk": "^4.1.0",
    "eslint-config-shellscape": "^5.0.2",
    "globby": "^11.0.1",
    "lint-staged": "^10.4.0",
    "nyc": "^15.1.0",
    "perfy": "^1.1.5",
    "postcss": "^8.4.14",
    "pre-commit": "^1.2.2",
    "rewiremock": "^3.14.3",
    "strip-ansi": "^6.0.0",
    "text-table": "^0.2.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "postcss": "^8.4.14"
  },
  "keywords": [
    "css",
    "less",
    "ast",
    "postcss",
    "value",
    "values",
    "parser",
    "parsing",
    "properties"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "nyc": {
    "include": [
      "lib/*.js"
    ],
    "exclude": [
      "test/"
    ]
  },
  "pre-commit": "lint-staged"
}
